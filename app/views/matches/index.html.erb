<p style="color: green"><%= notice %></p>

<h1>Matches</h1>

<div id="matches">
  <% @matches.each do |match| %>
    <%= render match %>
    <p>
      <%= link_to "Score this match", edit_match_match_action_path(match, match.match_action), class: "button" %>
    </p>
  <% end %>
</div>

<%= link_to "New match", new_match_path %>
