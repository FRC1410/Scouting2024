<p style="color: green"><%= notice %></p>

<h1>Matches</h1>

<%= link_to "New match", new_match_path, class: "button" %>
<table id="matches" class="hover">
  <thead>
  <tr>
    <th><strong>Number</strong></th>
    <th></th>
  </tr>
  </thead>
  <% @matches.each do |match| %>
    <%= render match %>
  <% end %>
</table>

