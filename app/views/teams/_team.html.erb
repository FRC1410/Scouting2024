<tr id="<%= dom_id team %>">
  <td>
    <%= link_to(team.number, team) %>
  </td>

  <td>
    <%= link_to(team.name, team) %>
  </td>

  <td>
    <%= link_to(team.location, team) %>
  </td>

  <td>
    <%= link_to(team.team_type, team) %>
